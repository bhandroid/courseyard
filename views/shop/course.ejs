    <script>
      var vbtn=document.getElementsByClassName('video-btn');
     
      for(btn of vbtn){
          
      btn.addEventListener("click", function(event){
        console.log( event.target.parentElement)
          const id='videoid';
          let num=parseInt(event.target.parentElement.id.substring(9));
        
          for(item of vbtn){
           item.classList.remove('uk-active');
          }
        
     
        var url="<%=urls%>";
        url=url.split(',');
        console.log(num,url);
         videojs("my-video", {}, function () {
                this.reset();
                this.src(url[num]);
                this.load();
                this.play();
            });
         console.log( document.getElementById(id).children);
          event.target.parentElement.classList.add('uk-active');
        
            })
      }


      var check='<%=check%>';
      if(check!=null && check !=undefined && check!='')
    check=check.split(',');
    else{
      var o=parseInt("<%=total%>");
      check=new Array(o).fill(0);
    console.log(check);
    }
    

    var checkbox=document.getElementsByClassName('checkk');
    for(cb of checkbox){
    cb.addEventListener('click',e=>{
      id=e.target.id;
      console.log(e.target)
      if(e.target.checked){
        check[id]=1;
      }else{
        check[id]=0;
      }
    
    console.log(check);
    fetch("/update_user", {
            method: "POST",
            body: JSON.stringify({
                _csrf: "<%= csrfToken%>",
                array: check,
                id:"<%=curriculum.course_name._id%>"
            }),
            headers: {
                "Content-type": "application/json; charset=UTF-8"
            }
        })
          
        .then(response =>{
          console.log(response)
          window.location.reload();
        })
        .catch(err=>console.log(err))
        
  
    })
  }
    </script>

